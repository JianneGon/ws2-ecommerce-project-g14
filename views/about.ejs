  <script>
    function toggleValue(index) {
      const items = document.querySelectorAll('.value-item');

      items.forEach((item, i) => {
        const icon = item.querySelector('.value-icon');
        const isActive = item.classList.contains('active');

        if (i === index) {
          items.forEach(i => i.classList.remove('active'));
          document.querySelectorAll('.value-icon').forEach(ic => ic.textContent = '+');

          if (!isActive) {
            item.classList.add('active');
            icon.textContent = '–';
          }
        }
      });
    }
  </script>

  <script>
document.addEventListener("DOMContentLoaded", () => {
  const years = document.querySelectorAll(".dot-year");
  const cards = document.querySelectorAll(".timeline-card");
  const total = years.length;
  let index = 0;

  function resetAnimation(el) {
    el.style.animation = "none";
    el.offsetHeight; // force reflow
    el.style.animation = null;
  }

  function showCard(i) {
    years.forEach((y, idx) => {
      y.classList.toggle("active", idx === i);
    });

    cards.forEach((card, idx) => {
      card.classList.remove("active");

      // RESET ANIMATIONS
      resetAnimation(card);
      resetAnimation(card.querySelector(".timeline-image-lg img"));
      resetAnimation(card.querySelector(".timeline-text h3"));
      resetAnimation(card.querySelector(".timeline-text p"));
    });

    // ACTIVATE CARD
    const activeCard = cards[i];
    activeCard.classList.add("active");
  }

  // Click year
  years.forEach((y, i) => {
    y.addEventListener("click", () => {
      index = i;
      showCard(index);
    });
  });

  // Arrow next
  document.querySelector(".next-year").addEventListener("click", () => {
    index = (index + 1) % total;
    showCard(index);
  });

  // Arrow previous
  document.querySelector(".prev-year").addEventListener("click", () => {
    index = (index - 1 + total) % total;
    showCard(index);
  });

  // Initial state
  showCard(0);
});


// TESTIMONIAL script//
let cmwIndex = 0;

const cmwSlider = document.querySelector(".cmw-slider");
const cmwCards = document.querySelectorAll(".cmw-testimonial-card");
const cmwBtnPrev = document.querySelector(".cmw-slider-btn.prev");
const cmwBtnNext = document.querySelector(".cmw-slider-btn.next");

function updateSlider() {
  const cardWidth = cmwCards[0].offsetWidth + 25;
  cmwSlider.style.transform = `translateX(-${cmwIndex * cardWidth}px)`;
}

cmwBtnNext.addEventListener("click", () => {
  if (cmwIndex < cmwCards.length - 3) cmwIndex++;
  updateSlider();
});

cmwBtnPrev.addEventListener("click", () => {
  if (cmwIndex > 0) cmwIndex--;
  updateSlider();
});

window.addEventListener("resize", updateSlider);


</script>


